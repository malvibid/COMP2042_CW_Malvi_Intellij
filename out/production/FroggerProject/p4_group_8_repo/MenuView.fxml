<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="p4_group_8_repo.MenuController">
    <children>
        <ImageView fitHeight="800.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@FroggerAssets/MenuBackground.png" />
            </image>
        </ImageView>
        <StackPane layoutX="200.0" layoutY="432.0" prefHeight="280.0" prefWidth="200.0">
            <children>
                <Button fx:id="Play" mnemonicParsing="false" onAction="#play" prefHeight="50.0" prefWidth="200.0" text="PLAY" translateY="-110.0">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Button>
                <Button fx:id="Highscore" layoutX="10.0" layoutY="123.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="HIGHSCORE" translateY="-40.0">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Button>
                <Button fx:id="Help" layoutX="10.0" layoutY="123.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="HELP" translateY="30.0">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Button>
                <Button fx:id="Exit" layoutX="10.0" layoutY="123.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="EXIT" translateY="100.0">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Button>
            </children></StackPane>
    </children>
</AnchorPane>
